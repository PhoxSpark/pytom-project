{
    "openapi": "3.0.0",
    "info": {
        "description": "Pytom project is an API made as a learning and practicing project for a student in the BSC.",
        "version": "3.0.0",
        "title": "Pytom Project",
        "contact": {
            "email": "luisgixd1997@gmail.com"
        },
        "license": {
            "name": "MIT",
            "url": "https://opensource.org/licenses/MIT"
        }
    },
    "servers": [
        {
            "url": "/"
        }
    ],
    "paths": {
        "/delete": {
            "delete": {
                "operationId": "delData",
                "summary": "Delete data",
                "description": "Delete data from dictionary or database.",
                "parameters": [
                    {
                        "name": "delete",
                        "required": false,
                        "in": "path",
                        "type": "string",
                        "description": "Write database or dictionary depending on what you want to delete, leave empty for delete both."
                    }
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    200: {
                        "description": "200 response",
                        "examples": {
                            "application/json": "Success!"
                        }
                    }
                }
            }
        }
    }
}
    